{"ast":null,"code":"import \"core-js/modules/es.array.includes.js\";\nimport { getAPI } from '../axios-api';\nimport tagBox from '../components/TagBox';\nexport default {\n  name: 'Upload',\n\n  data() {\n    return {\n      genres: [],\n      tags: [],\n      selectedtags: [],\n      selectedgenres: []\n    };\n  },\n\n  components: {\n    tagBox\n  },\n  methods: {\n    addTag(tag) {\n      const tagbutton = document.getElementById(tag);\n      tagbutton.classList.remove('unselected');\n\n      if (!this.selectedtags.includes(tag)) {\n        this.selectedtags.push(tag);\n      }\n\n      console.log(this.selectedtags);\n    },\n\n    removeTag(removetag) {\n      console.log(removetag);\n      const index = this.selectedtags.indexOf(removetag);\n      this.selectedtags.splice(index, 1);\n      document.getElementById(removetag).classList.add('unselected');\n    },\n\n    filterTag() {\n      console.log(document.getElementById('tagfilter'));\n      tagbuttons = document.getElementByClassName('tagbutton');\n\n      for (i = 0; i < tagbuttons.length; i++) {}\n    },\n\n    postData() {\n      const loadingscreen = document.getElementById('loadingscreen');\n      loadingscreen.style.display = 'block';\n      const form = document.getElementById('download');\n      const selectedGenres = 'Fantasy, Comedy'; //form['genres'].value\n\n      const url = form['url'].value;\n      console.log('posted');\n      getAPI.post('/novel/single', {\n        url: url,\n        tags: this.selectedtags,\n        genres: selectedGenres\n      }).then(function (response) {\n        console.log(response);\n        loadingscreen.style.display = 'none';\n      }).catch(function (error) {\n        console.log(error);\n        loadingscreen.style.display = 'none';\n      });\n    }\n\n  },\n\n  created() {\n    getAPI.get('/uploaddata').then(response => {\n      console.log('Chapter API has recieved data');\n      this.genres = response.data['genres'];\n      this.tags = response.data['tags'];\n    }).catch(err => {\n      console.log(err);\n    });\n  }\n\n};","map":{"version":3,"mappings":";AAqCA,SAASA,MAAT,QAAuB,cAAvB;AACA,OAAOC,MAAP,MAAmB,sBAAnB;AAEA,eAAc;EACVC,IAAI,EAAC,QADK;;EAEVC,IAAI,GAAE;IACF,OAAM;MACFC,MAAM,EAAC,EADL;MAEFC,IAAI,EAAC,EAFH;MAGFC,YAAY,EAAC,EAHX;MAIFC,cAAc,EAAC;IAJb,CAAN;EAMH,CATS;;EAUVC,UAAU,EAAC;IACPP;EADO,CAVD;EAaVQ,OAAO,EAAC;IACJC,MAAM,CAACC,GAAD,EAAK;MACP,MAAMC,SAAQ,GAAIC,QAAQ,CAACC,cAAT,CAAwBH,GAAxB,CAAlB;MACAC,SAAS,CAACG,SAAV,CAAoBC,MAApB,CAA2B,YAA3B;;MACA,IAAG,CAAC,KAAKV,YAAL,CAAkBW,QAAlB,CAA2BN,GAA3B,CAAJ,EAAoC;QAChC,KAAKL,YAAL,CAAkBY,IAAlB,CAAuBP,GAAvB;MACJ;;MACAQ,OAAO,CAACC,GAAR,CAAY,KAAKd,YAAjB;IACH,CARG;;IASJe,SAAS,CAACC,SAAD,EAAW;MAChBH,OAAO,CAACC,GAAR,CAAYE,SAAZ;MACA,MAAMC,KAAI,GAAI,KAAKjB,YAAL,CAAkBkB,OAAlB,CAA0BF,SAA1B,CAAd;MACA,KAAKhB,YAAL,CAAkBmB,MAAlB,CAAyBF,KAAzB,EAA+B,CAA/B;MACAV,QAAQ,CAACC,cAAT,CAAwBQ,SAAxB,EAAmCP,SAAnC,CAA6CW,GAA7C,CAAiD,YAAjD;IACH,CAdG;;IAeJC,SAAS,GAAE;MACPR,OAAO,CAACC,GAAR,CAAYP,QAAQ,CAACC,cAAT,CAAwB,WAAxB,CAAZ;MACAc,UAAS,GAAIf,QAAQ,CAACgB,qBAAT,CAA+B,WAA/B,CAAb;;MACA,KAAIC,IAAI,CAAR,EAAWA,IAAIF,UAAU,CAACG,MAA1B,EAAkCD,CAAC,EAAnC,EAAsC,CAEtC;IACH,CArBG;;IAsBJE,QAAQ,GAAE;MACN,MAAMC,aAAY,GAAIpB,QAAQ,CAACC,cAAT,CAAwB,eAAxB,CAAtB;MACAmB,aAAa,CAACC,KAAd,CAAoBC,OAApB,GAA8B,OAA9B;MACA,MAAMC,IAAG,GAAIvB,QAAQ,CAACC,cAAT,CAAwB,UAAxB,CAAb;MACA,MAAMuB,cAAa,GAAI,iBAAvB,CAJM,CAIkC;;MACxC,MAAMC,GAAE,GAAIF,IAAI,CAAC,KAAD,CAAJ,CAAYG,KAAxB;MACApB,OAAO,CAACC,GAAR,CAAY,QAAZ;MACApB,MAAM,CAACwC,IAAP,CAAY,eAAZ,EAA6B;QAACF,GAAG,EAACA,GAAL;QAASjC,IAAI,EAAC,KAAKC,YAAnB;QAAgCF,MAAM,EAACiC;MAAvC,CAA7B,EACKI,IADL,CACU,UAAUC,QAAV,EAAoB;QACtBvB,OAAO,CAACC,GAAR,CAAYsB,QAAZ;QACAT,aAAa,CAACC,KAAd,CAAoBC,OAApB,GAA8B,MAA9B;MACP,CAJD,EAKKQ,KALL,CAKW,UAAUC,KAAV,EAAiB;QACpBzB,OAAO,CAACC,GAAR,CAAYwB,KAAZ;QACAX,aAAa,CAACC,KAAd,CAAoBC,OAApB,GAA8B,MAA9B;MACP,CARD;IASJ;;EAtCI,CAbE;;EAqDVU,OAAO,GAAE;IACL7C,MAAM,CAAC8C,GAAP,CAAW,aAAX,EACGL,IADH,CACQC,QAAO,IAAK;MAChBvB,OAAO,CAACC,GAAR,CAAY,+BAAZ;MACA,KAAKhB,MAAL,GAAcsC,QAAQ,CAACvC,I