nu.$refs.shade.style.opacity='0.5';\n            menuBtn.classList.add('open');\n          }\n          \n        },\n        open_signin(){\n          //this.$refs.signin.$refs.signin.style.display='block'\n          this.switchdarkmode()\n          console.log('open')\n        },\n        close_signin(){\n          this.$refs.signin.$refs.signin.style.display='none'\n        },\n        signup(){\n           this.$refs.signin.$refs.signin.style.display='block'\n        },\n        switchdarkmode(){\n          var r = document.querySelector(':root');\n          r.style.setProperty('--backgroundColor', '#262626')\n          if(darkmode){\n           r.style.setProperty('--backgroundColor', 'white')\n           r.style.setProperty('--textColor', 'black')\n          }else{\n            r.style.setProperty('--backgroundColor', 'black')\n            r.style.setProperty('--textColor', 'white')\n          }\n\n        }\n            \n        },\n    errorCaptured: function(err) {\n      //this.$router.push({name:'errorPage'})\n      \n    },\n    created(){\n      const width = document.getElementById('app').style;\n      console.log(width)\n\n    }\n  \n}\n</script>\n\n<style>\n:root{\n  --styleColor: #75147C;\n  --backgroundColor: #FFFFFF;\n  --textColor: #000000;\n}\n*{\n  font:Arial, Helvetica;\n  color:var(--textColor);\n}\n#app{\n  display:grid;\n  grid-template-columns: auto auto auto;\n  padding:0px;\n  width:100%;\n  gap:0px;\n}\n  body {\n    margin: 0;\n    padding: 0;\n    background-color:var(--backgroundColor)!important;\n    \n  }\n  .content{\n      position:relative;\n      height: 100%;\n      width:100%;\n      \n  }\n  .view-header{\n    grid-row:1;\n    grid-column: 1 / span 3;\n    height:fit-content;\n    width:100%;\n  }\n  .view-border-one{\n   grid-row:2;\n   grid-column:1;\n  }\n  .view-content{\n      grid-row:2;\n      grid-column:2;\n      margin:auto;\n      width:100%;\n      max-width:1200px;\n  }\n  .view-border-two{\n    grid-row:2;\n    grid-column:3;\n  }\n  .view-footer:{\n    grid-row: 3;\n    grid-column: 1 / span 3;\n  }\n  .border{\n    height:100%;\n    border: 1px green solid;\n    margin:0px;\n    width:100%;\n    background-color:red;\n\n  }\n\n  @media (max-width: 1200px){\n     .view-content{\n        width:92.5%;\n         transition: all .3s ease;\n     }\n  }\n\n@media (max-width: 775px) {\n    .content{\n        display: flex;\n        flex-direction: column;\n        margin-top: 30px;\n        /*margin: auto;*/\n        transition: all .3s ease;\n    }\n}\n</style>\n"]},"metadata":{},"sourceType":"module"}