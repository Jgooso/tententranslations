{"ast":null,"code":"import { getAPI } from '../axios-api';\nexport default {\n  name: 'Schedular',\n\n  data() {\n    return {\n      scheduleList: []\n    };\n  },\n\n  methods: {\n    timeConverter(time) {\n      var convertedTime = parseInt(time);\n\n      if ((convertedTime %= 12) == convertedTime) {\n        (convteredTime %= 12).toString() + 'AM';\n      } else {\n        (convteredTime %= 12).toString() + 'PM';\n      }\n    }\n\n  },\n\n  created() {\n    getAPI.get('/schedule').then(response => {\n      console.log('Schedule API has recieved data');\n      console.log(response);\n      this.scheduleList = response.data;\n    }).catch(err => {\n      console.log(err);\n    });\n  }\n\n};","map":{"version":3,"mappings":"AAmBA,SAAQA,MAAR,QAAqB,cAArB;AACA,eAAc;EACVC,IAAI,EAAC,WADK;;EAEVC,IAAI,GAAE;IACF,OAAM;MACFC,YAAY,EAAC;IADX,CAAN;EAGH,CANS;;EAOVC,OAAO,EAAC;IACJC,aAAa,CAACC,IAAD,EAAM;MACf,IAAIC,aAAY,GAAIC,QAAQ,CAACF,IAAD,CAA5B;;MACA,IAAG,CAACC,aAAY,IAAK,EAAlB,KAAyBA,aAA5B,EAA0C;QACtC,CAACE,aAAY,IAAK,EAAlB,EAAsBC,QAAtB,KAAmC,IAAnC;MACH,CAFD,MAEK;QACD,CAACD,aAAY,IAAK,EAAlB,EAAsBC,QAAtB,KAAmC,IAAnC;MACJ;IACJ;;EARI,CAPE;;EAiBVC,OAAO,GAAE;IACLX,MAAM,CAACY,GAAP,CAAW,WAAX,EACGC,IADH,CACQC,QAAO,IAAK;MAChBC,OAAO,CAACC,GAAR,CAAY,gCAAZ;MACAD,OAAO,CAACC,GAAR,CAAYF,QAAZ;MACA,KAAKX,YAAL,GAAkBW,QAAQ,CAACZ,IAA3B;IACD,CALH,EAMGe,KANH,CAMSC,GAAE,IAAK;MACZH,OAAO,CAACC,GAAR,CAAYE,GAAZ;IACD,CARH;EASR;;AA3Bc,CAAd","names":["getAPI","name","data","scheduleList","methods","timeConverter","time","convertedTime","parseInt","convteredTime","toString","created","get","then","response","console","log","catch","err"],"sourceRoot":"","sources":["/Users/joshgoodman/Documents/GitHub/tententranslations/vuefrontend/src/components/Schedular.vue"],"sourcesContent":["<template>\n<div class = 'schedular'>\n<ul>\n<li v-for='(scheduleDay,day) in scheduleList' :key='day'>\n{{day}}\n<ul>\n    <li v-for='schedule in scheduleDay' :key='schedule.day+schedule.time'>\n        <div>\n            {{timeConverter(schedule.time)}}\n            {{schedule.title}}\n        </div>\n    </li>\n    </ul>\n</li>\n</ul>\n</div>\n</template>\n\n<script>\nimport {getAPI} from '../axios-api'\nexport default{\n    name:'Schedular',\n    data(){\n        return{\n            scheduleList:[],\n        }\n    },\n    methods:{\n        timeConverter(time){\n            var convertedTime = parseInt(time)\n            if((convertedTime %= 12) == convertedTime){\n                (convteredTime %= 12).toString() + 'AM'\n            }else{\n                (convteredTime %= 12).toString() + 'PM'\n            }\n        }\n    },\n    created(){\n        getAPI.get('/schedule')\n          .then(response => {\n            console.log('Schedule API has recieved data')\n            console.log(response)\n            this.scheduleList=response.data\n          })\n          .catch(err => {\n            console.log(err)\n          })\n}\n}\n</script>\n\n<style scoped>\n</style>"]},"metadata":{},"sourceType":"module"}