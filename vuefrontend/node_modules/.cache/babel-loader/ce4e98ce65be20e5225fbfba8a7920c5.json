{"ast":null,"code":"import Header from './components/Header';\nimport Footer from './components/Footer';\nimport sidebar from './components/Sidebar';\nimport SignIn from './components/SignIn';\nimport { getAPI } from './axios-api';\nexport default {\n  name: 'App',\n  components: {\n    Header,\n    Footer,\n    sidebar,\n    SignIn\n  },\n\n  data() {\n    return {\n      darkmode: false,\n      testing: [],\n      user: []\n    };\n  },\n\n  methods: {\n    changesidebar() {\n      const menuBtn = this.$refs.header.$refs.menubutton;\n\n      if (this.$refs.sidemenu.$refs.sidebar.style.left == '0px') {\n        this.$refs.sidemenu.$refs.sidebar.style.left = '-300px';\n        this.$refs.sidemenu.$refs.shade.style.opacity = '0';\n        menuBtn.classList.remove('open');\n      } else {\n        this.$refs.sidemenu.$refs.sidebar.style.left = '0px';\n        this.$refs.sidemenu.$refs.shade.style.opacity = '0.5';\n        menuBtn.classList.add('open');\n      }\n    },\n\n    open_signin() {\n      //this.$refs.signin.$refs.signin.style.display='block'\n      this.switchdarkmode();\n      console.log('open');\n    },\n\n    close_signin() {\n      this.$refs.signin.$refs.signin.style.display = 'none';\n    },\n\n    signup() {\n      this.$refs.signin.$refs.signin.style.display = 'block';\n    },\n\n    switchdarkmode() {\n      var r = document.querySelector(':root');\n\n      if (this.darkmode) {\n        r.style.setProperty('--backgroundColor', 'white');\n        r.style.setProperty('--textColor', 'black');\n        r.style.setProperty('--shadowColor', 'rgba(0,0,0,0.19)');\n        r.style.setProperty('--borderColor', 'black');\n        r.style.setProperty('--styleColor', '#75147C');\n      } else {\n        r.style.setProperty('--backgroundColor', '#262626');\n        r.style.setProperty('--textColor', '#959595');\n        r.style.setProperty('--shadowColor', 'rgba(255,255,255,0.19)');\n        r.style.setProperty('--borderColor', 'white');\n        r.style.setProperty('--styleColor', '#A837AF');\n      }\n\n      this.darkmode = !this.darkmode;\n    }\n\n  },\n  errorCaptured: function (err) {//this.$router.push({name:'errorPage'})\n  },\n\n  created() {\n    try {\n      r.style.setProperty('--fontsize', $cookies.get('font-size') + 'px');\n    } catch (e) {\n      console.log(e);\n      r.style.setProperty('--fontsize', '19px');\n    }\n    /*\n    const userID = $cookies.get('user')\n    console.log(userID)\n    if(userID==null){\n      const id = parseInt(Math.random()*1000000)\n      $cookies.set('user',id)\n      getAPI.post('/user',{userID:id})\n          .then(response => {\n          console.log(response)\n        })\n        .catch(err => {\n          console.log(err)\n        })\n    }else{\n          getAPI.get('/user?userID='+userID)\n          .then(response => {\n          \n          console.log(response['data'])\n          this.user = response['data']\n          console.log(this.user)\n          var r = document.querySelector(':root');\n          r.style.setProperty('--fontsize', this.user.fontsize+'px')\n        })\n        .catch(err => {\n          console.log(err)\n        })\n    */\n\n\n    this.user = {\n      'id': 838383,\n      'status': 'Moderator',\n      'fontsize': 19,\n      'tier': 1\n    };\n  }\n\n};","map":{"version":3,"mappings":"AA2BA,OAAOA,MAAP,MAAmB,qBAAnB;AACA,OAAOC,MAAP,MAAmB,qBAAnB;AACA,OAAOC,OAAP,MAAoB,sBAApB;AACA,OAAOC,MAAP,MAAmB,qBAAnB;AACA,SAASC,MAAT,QAAuB,aAAvB;AACA,eAAe;EAEbC,IAAI,EAAE,KAFO;EAGbC,UAAU,EAAC;IACTN,MADS;IAETC,MAFS;IAGTC,OAHS;IAITC;EAJS,CAHE;;EASbI,IAAI,GAAE;IACJ,OAAM;MACNC,QAAQ,EAAC,KADH;MAENC,OAAO,EAAC,EAFF;MAGNC,IAAI,EAAE;IAHA,CAAN;EAKD,CAfY;;EAgBbC,OAAO,EAAC;IACFC,aAAa,GAAE;MACb,MAAMC,OAAM,GAAI,KAAKC,KAAL,CAAWC,MAAX,CAAkBD,KAAlB,CAAwBE,UAAxC;;MACA,IAAG,KAAKF,KAAL,CAAWG,QAAX,CAAoBH,KAApB,CAA0BZ,OAA1B,CAAkCgB,KAAlC,CAAwCC,IAAxC,IAA8C,KAAjD,EAAuD;QACrD,KAAKL,KAAL,CAAWG,QAAX,CAAoBH,KAApB,CAA0BZ,OAA1B,CAAkCgB,KAAlC,CAAwCC,IAAxC,GAA6C,QAA7C;QACA,KAAKL,KAAL,CAAWG,QAA