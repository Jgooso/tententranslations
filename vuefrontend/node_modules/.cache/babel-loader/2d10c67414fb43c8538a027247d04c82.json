{"ast":null,"code":"import { getAPI } from '../axios-api';\nexport default {\n  name: 'Novel',\n  components: {},\n\n  data() {\n    return {\n      chapterList: [],\n      sectionList: [],\n      novelData: [],\n      isMounted: false\n    };\n  },\n\n  props: ['tier', 'editable', 'novel'],\n\n  created() {\n    if (this.editable) {\n      const url = '/novel/single?novel=' + this.novel + '&tier=' + this.tier;\n    } else {\n      const url = '/novel/single?novel=' + this.$route.params.title + '&tier=' + this.tier;\n    }\n\n    getAPI.get(url).then(response => {\n      console.log('Post Novel has recieved data');\n      this.novelData = response.data['Novel'];\n      this.sectionList = response.data['Chapters'].filter(chapter => chapter.chapternumber == 0);\n      this.chapterList = response.data['Chapters'].filter(chapter => chapter.chapternumber != 0);\n      this.isMounted = true;\n    }).catch(err => {\n      console.log(err);\n    });\n    console.log(this.$route.params);\n  }\n\n};","map":{"version":3,"mappings":"AAaE,SAASA,MAAT,QAAuB,cAAvB;AACA,eAAe;EACbC,IAAI,EAAE,OADO;EAEbC,UAAU,EAAE,EAFC;;EAIbC,IAAG,GAAK;IACN,OAAO;MACHC,WAAW,EAAC,EADT;MAEHC,WAAW,EAAC,EAFT;MAGHC,SAAS,EAAC,EAHP;MAIHC,SAAS,EAAC;IAJP,CAAP;EAMD,CAXY;;EAYbC,KAAK,EAAC,CACJ,MADI,EAEJ,UAFI,EAGJ,OAHI,CAZO;;EAiBbC,OAAM,GAAK;IACX,IAAG,KAAKC,QAAR,EAAiB;MACf,MAAMC,GAAE,GAAI,yBAAuB,KAAKC,KAA5B,GAAkC,QAAlC,GAA2C,KAAKC,IAA5D;IACD,CAFD,MAEK;MACH,MAAMF,GAAE,GAAI,yBAAuB,KAAKG,MAAL,CAAYC,MAAZ,CAAmBC,KAA1C,GAAgD,QAAhD,GAAyD,KAAKH,IAA1E;IACF;;IACQb,MAAM,CAACiB,GAAP,CAAWN,GAAX,EACDO,IADC,CACIC,QAAO,IAAK;MAChBC,OAAO,CAACC,GAAR,CAAY,8BAAZ;MACA,KAAKf,SAAL,GAAea,QAAQ,CAAChB,IAAT,CAAc,OAAd,CAAf;MACA,KAAKE,WAAL,GAAiBc,QAAQ,CAAChB,IAAT,CAAc,UAAd,EAA0BmB,MAA1B,CAAiCC,OAAM,IAAKA,OAAO,CAACC,aAAR,IAAyB,CAArE,CAAjB;MACA,KAAKpB,WAAL,GAAiBe,QAAQ,CAAChB,IAAT,CAAc,UAAd,EAA0BmB,MAA1B,CAAiCC,OAAM,IAAKA,OAAO,CAACC,aAAR,IAAyB,CAArE,CAAjB;MACA,KAAKjB,SAAL,GAAe,IAAf;IACD,CAPC,EAQDkB,KARC,CAQKC,GAAE,IAAK;MACZN,OAAO,CAACC,GAAR,CAAYK,G