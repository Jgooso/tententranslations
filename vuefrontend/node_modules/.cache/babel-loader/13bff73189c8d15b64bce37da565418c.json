{"ast":null,"code":"import { getAPI } from '../axios-api';\nexport default {\n  name: 'Calender',\n\n  data() {\n    return {\n      dates: [],\n      daysoftheWeek: ['Sunday', 'Monday', 'Tuesady', 'Wednesday', 'Thursday', 'Friday', 'Saturday'],\n      month: '',\n      day: 0\n    };\n  },\n\n  methods: {\n    print(a) {\n      console.log(a);\n    }\n\n  },\n\n  created() {\n    getAPI.get('/dates').then(response => {\n      console.log('Chapter API has recieved data');\n      this.dates = response.data;\n      const start_date = new Date(this.dates[1]);\n      const end_date = new Date(this.dates[-1]);\n      this.month = start_date.toLocaleString('default', {\n        month: 'long',\n        weekday: 'narrow'\n      });\n      console.log(start_date);\n      this.day = start_date.getDay();\n      this.endDay = 7 - end_date.getDay();\n      console.log(this.month);\n      console.log(this.day);\n    }).catch(err => {\n      console.log(err);\n    });\n  }\n\n};","map":{"version":3,"mappings":"AAmBA,SAASA,MAAT,QAAuB,cAAvB;AACA,eAAc;EACVC,IAAI,EAAC,UADK;;EAEVC,IAAI,GAAE;IACF,OAAO;MACHC,KAAK,EAAC,EADH;MAEHC,aAAa,EAAC,CAAC,QAAD,EAAU,QAAV,EAAmB,SAAnB,EAA6B,WAA7B,EAAyC,UAAzC,EAAoD,QAApD,EAA6D,UAA7D,CAFX;MAGHC,KAAK,EAAC,EAHH;MAIHC,GAAG,EAAC;IAJD,CAAP;EAMH,CATS;;EAUVC,OAAO,EAAC;IACJC,KAAK,CAACC,CAAD,EAAG;MACJC,OAAO,CAACC,GAAR,CAAYF,CAAZ;IACJ;;EAHI,CAVE;;EAeVG,OAAO,GAAE;IACLZ,MAAM,CAACa,GAAP,CAAW,QAAX,EACGC,IADH,CACQC,QAAO,IAAK;MAChBL,OAAO,CAACC,GAAR,CAAY,+BAAZ;MACA,KAAKR,KAAL,GAAaY,QAAQ,CAACb,IAAtB;MACA,MAAMc,UAAS,GAAI,IAAIC,IAAJ,CAAS,KAAKd,KAAL,CAAW,CAAX,CAAT,CAAnB;MACA,MAAMe,QAAO,GAAI,IAAID,IAAJ,CAAS,KAAKd,KAAL,CAAW,CAAC,CAAZ,CAAT,CAAjB;MACA,KAAKE,KAAL,GAAaW,UAAU,CAACG,cAAX,CAA0B,SAA1B,EAAqC;QAAEd,KAAK,EAAE,MAAT;QAAgBe,OAAO,EAAC;MAAxB,CAArC,CAAb;MACAV,OAAO,CAACC,GAAR,CAAYK,UAAZ;MACA,KAAKV,GAAL,GAAWU,UAAU,CAACK,MAAX,EAAX;MACA,KAAKC,MAAL,GAAc,IAAEJ,QAAQ,CAACG,MAAT,EAAhB;MACAX,OAAO,CAACC,GAAR,CAAY,KAAKN,KAAjB;MACAK,OAAO,CAACC,GAAR,CAAY,KAAKL,GAAjB;IACD,CAZH,EAaGiB,KAbH,CA