{"ast":null,"code":"import ChapterSelector from '../components/ChapterSelector';\nimport Navigator from '../components/Navigator';\nimport { getAPI } from '../axios-api';\nexport default {\n  name: 'Chapter',\n  components: {\n    ChapterSelector,\n    Navigator\n  },\n  props: ['novelData', 'chapterList', 'chapter'],\n\n  data() {\n    return {\n      chapter: this.chapterList[this.$route.params.chapter - 1]\n    };\n  },\n\n  methods: {\n    changeFontSize(change) {\n      if (change == 1) {\n        document.getElementsByTagName('pre')[0].style.fontSize = 'larger';\n      } else {\n        document.getElementsByTagName('pre')[0].style.fontSize = 'smaller';\n      }\n    }\n\n  },\n\n  created() {\n    const url = '/chapter?novel=' + novel + '&chapter=' + chapternumber;\n    getAPI.get(url).then(response => {\n      console.log('Chapter API has recieved data');\n      this.chapter = response.data;\n      console.log(this.chapter);\n    }).catch(err => {\n      console.log(err);\n    });\n    this.chapterList.sort((a, b) => a.chapterNumber > b.chapterNumber ? 1 : -1);\n  }\n\n};","map":{"version":3,"mappings":"AA2BA,OAAOA,eAAP,MAA4B,+BAA5B;AACA,OAAOC,SAAP,MAAsB,yBAAtB;AACA,SAASC,MAAT,QAAuB,cAAvB;AACI,eAAc;EACVC,IAAI,EAAE,SADI;EAEVC,UAAU,EAAC;IACPJ,eADO;IAEPC;EAFO,CAFD;EAKRI,KAAK,EAAC,CACR,WADQ,EAER,aAFQ,EAGR,SAHQ,CALE;;EASRC,IAAI,GAAE;IACJ,OAAM;MACFC,OAAO,EAAC,KAAKC,WAAL,CAAiB,KAAKC,MAAL,CAAYC,MAAZ,CAAmBH,OAAnB,GAA2B,CAA5C;IADN,CAAN;EAGH,CAbS;;EAaRI,OAAO,EAAC;IACNC,cAAc,CAACC,MAAD,EAAQ;MAClB,IAAGA,MAAK,IAAK,CAAb,EAAe;QACXC,QAAQ,CAACC,oBAAT,CAA8B,KAA9B,EAAqC,CAArC,EAAwCC,KAAxC,CAA8CC,QAA9C,GAAuD,QAAvD;MACH,CAFD,MAEK;QACDH,QAAQ,CAACC,oBAAT,CAA8B,KAA9B,EAAqC,CAArC,EAAwCC,KAAxC,CAA8CC,QAA9C,GAAuD,SAAvD;MACJ;IACJ;;EAPM,CAbA;;EAsBVC,OAAO,GAAE;IAED,MAAMC,GAAE,GAAI,oBAAkBC,KAAlB,GAAwB,WAAxB,GAAoCC,aAAhD;IACJnB,MAAM,CAACoB,GAAP,CAAWH,GAAX,EACDI,IADC,CACIC,QAAO,IAAK;MAChBC,OAAO,CAACC,GAAR,CAAY,+BAAZ;MACA,KAAKnB,OAAL,GAAeiB,QAAQ,CAAClB,IAAxB;MACAmB,OAAO,CAACC,GAAR,CAAY,KAAKnB