{"ast":null,"code":"import axios from 'axios';\nexport default {\n  name: 'Novel',\n  components: {},\n\n  data() {\n    return {\n      chapterList: [],\n      sectionList: [],\n      novelData: [],\n      isMounted: false\n    };\n  },\n\n  props: ['tier', 'url'],\n\n  created() {\n    const get_url = `http://${url}/novel/single?novel=${this.$route.params.title}&tier=${this.tier}`;\n    axios.get(get_url).then(response => {\n      this.novelData = response.data['Novel'];\n      const sections = response.data['Chapters'].filter(chapter => chapter.chapternumber == 0);\n\n      if (sections.length > 0) {\n        this.sectionList = sections;\n      } else {\n        this.sectionList = [{\n          'section': 0\n        }];\n      }\n\n      this.chapterList = response.data['Chapters'].filter(chapter => chapter.chapternumber != 0);\n      this.isMounted = true;\n    }).catch(err => {\n      console.log(err);\n    });\n  }\n\n};","map":{"version":3,"mappings":"AAcE,OAAOA,KAAP,MAAkB,OAAlB;AACA,eAAe;EACbC,IAAI,EAAE,OADO;EAEbC,UAAU,EAAE,EAFC;;EAIbC,IAAG,GAAK;IACN,OAAO;MACHC,WAAW,EAAC,EADT;MAEHC,WAAW,EAAC,EAFT;MAGHC,SAAS,EAAC,EAHP;MAIHC,SAAS,EAAC;IAJP,CAAP;EAMD,CAXY;;EAYbC,KAAK,EAAC,CACJ,MADI,EAEJ,KAFI,CAZO;;EAgBbC,OAAM,GAAK;IACX,MAAMC,OAAM,GAAK,UAASC,GAAI,uBAAsB,KAAKC,MAAL,CAAYC,MAAZ,CAAmBC,KAAM,SAAQ,KAAKC,IAAK,EAA/F;IACQf,KAAK,CAACgB,GAAN,CAAUN,OAAV,EACDO,IADC,CACIC,QAAO,IAAK;MAChB,KAAKZ,SAAL,GAAeY,QAAQ,CAACf,IAAT,CAAc,OAAd,CAAf;MACD,MAAMgB,QAAO,GAAID,QAAQ,CAACf,IAAT,CAAc,UAAd,EAA0BiB,MAA1B,CAAiCC,OAAM,IAAKA,OAAO,CAACC,aAAR,IAAyB,CAArE,CAAjB;;MACC,IAAGH,QAAQ,CAACI,MAAT,GAAkB,CAArB,EAAuB;QACnB,KAAKlB,WAAL,GAAmBc,QAAnB;MACH,CAFD,MAEK;QACD,KAAKd,WAAL,GAAmB,CAAC;UAAC,WAAU;QAAX,CAAD,CAAnB;MACJ;;MACA,KAAKD,WAAL,GAAiBc,QAAQ,CAACf,IAAT,CAAc,UAAd,EAA0BiB,MAA1B,CAAiCC,OAAM,IAAKA,OAAO,CAACC,aAAR,IAAyB,CAArE,CAAjB;MACA,KAAKf,SAAL,GAAe,IAAf;IACD,CAXC,EAYDiB,KAZC,CAYKC,GAAE,IAAK;MACZC,OAAO,CAACC,GAAR,CAAYF,GAAZ;IACD,CAdC;EAeT;;AAjCc,CAAf","names":["axios","name","components","data","