{"ast":null,"code":"import ChapterSelector from '../components/ChapterSelector';\nimport Navigator from '../components/Navigator';\nimport { StripeElementCard } from '@vue-stripe/vue-stripe';\nexport default {\n  name: 'Chapter',\n  components: {\n    ChapterSelector,\n    Navigator,\n    StripeElementCard\n  },\n  props: ['novelData', 'chapterList'],\n\n  data() {\n    publishableKey = process.env.STRIPE_PUBLISHABLE_KEY;\n    return {\n      chapterNum: this.$route.params.chapter - 1,\n      token: null\n    };\n  },\n\n  methods: {\n    updateContent(chp) {\n      this.chapterNum = chp - 1;\n      window.scrollTo({\n        top: 0,\n        left: 0,\n        behavoir: 'smooth'\n      });\n    },\n\n    tokenCreated(token) {\n      console.log(token); // handle the token\n      // send it to your server\n    }\n\n  },\n\n  created() {\n    this.chapterList.sort((a, b) => a.chapterNumber > b.chapterNumber ? 1 : -1);\n  }\n\n};","map":{"version":3,"mappings":"AAgCA,OAAOA,eAAP,MAA4B,+BAA5B;AACA,OAAOC,SAAP,MAAsB,yBAAtB;AACA,SAASC,iBAAT,QAAkC,wBAAlC;AACI,eAAc;EACVC,IAAI,EAAE,SADI;EAEVC,UAAU,EAAC;IACPJ,eADO;IAEPC,SAFO;IAGPC;EAHO,CAFD;EAMRG,KAAK,EAAC,CACR,WADQ,EAER,aAFQ,CANE;;EAUVC,IAAG,GAAK;IACJC,cAAa,GAAIC,OAAO,CAACC,GAAR,CAAYC,sBAA7B;IACA,OAAO;MACHC,UAAU,EAAE,KAAKC,MAAL,CAAYC,MAAZ,CAAmBC,OAAnB,GAA2B,CADpC;MAEHC,KAAK,EAAC;IAFH,CAAP;EAIH,CAhBS;;EAgBRC,OAAO,EAAC;IACVC,aAAa,CAACC,GAAD,EAAK;MACd,KAAKP,UAAL,GAAkBO,GAAG,GAAC,CAAtB;MACAC,MAAM,CAACC,QAAP,CAAgB;QAACC,GAAG,EAAC,CAAL;QAAOC,IAAI,EAAC,CAAZ;QAAcC,QAAQ,EAAC;MAAvB,CAAhB;IAEH,CALS;;IAMVC,YAAW,CAAGT,KAAH,EAAU;MACvBU,OAAO,CAACC,GAAR,CAAYX,KAAZ,EADuB,CAEvB;MACA;IACD;;EAVa,CAhBA;;EA4BdY,OAAO,GAAE;IACL,KAAKC,WAAL,CAAiBC,IAAjB,CAAsB,CAACC,CAAD,EAAGC,CAAH,KAASD,CAAC,CAACE,aAAF,GAAkBD,CAAC,CAACC,aAApB,GAAoC,CAApC,GAAsC,CAAC,CAAtE;EACJ;;AA9Bc,CAAd","names":["ChapterSelector","Navigator","StripeElementCard","name","components","props","data","publishableKey","process","env","STRIPE_PUBLISHABLE_KEY","chapterNum","$route","params",