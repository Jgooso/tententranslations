{"ast":null,"code":"import { getAPI } from '../axios-api';\nexport default {\n  name: 'Calender',\n\n  data() {\n    return {\n      dates: [],\n      daysoftheWeek: ['Sunday', 'Monday', 'Tuesady', 'Wednesday', 'Thursday', 'Friday', 'Saturday'],\n      month: '',\n      day: 0,\n      selectedDay: []\n    };\n  },\n\n  methods: {\n    print(a) {\n      console.log(a);\n      this.selectedDay = a;\n    }\n\n  },\n\n  created() {\n    getAPI.get('/dates').then(response => {\n      console.log('Chapter API has recieved data');\n      this.dates = response.data;\n      const start_date = new Date(this.dates[1]);\n      this.month = start_date.toLocaleString('default', {\n        month: 'long'\n      });\n      console.log(start_date);\n      this.day = start_date.getDay();\n      console.log(this.month);\n      console.log(this.day);\n    }).catch(err => {\n      console.log(err);\n    });\n  }\n\n};","map":{"version":3,"mappings":"AAkBA,SAASA,MAAT,QAAuB,cAAvB;AACA,eAAc;EACVC,IAAI,EAAC,UADK;;EAEVC,IAAI,GAAE;IACF,OAAO;MACHC,KAAK,EAAC,EADH;MAEHC,aAAa,EAAC,CAAC,QAAD,EAAU,QAAV,EAAmB,SAAnB,EAA6B,WAA7B,EAAyC,UAAzC,EAAoD,QAApD,EAA6D,UAA7D,CAFX;MAGHC,KAAK,EAAC,EAHH;MAIHC,GAAG,EAAC,CAJD;MAKHC,WAAW,EAAC;IALT,CAAP;EAOH,CAVS;;EAWVC,OAAO,EAAC;IACJC,KAAK,CAACC,CAAD,EAAG;MACJC,OAAO,CAACC,GAAR,CAAYF,CAAZ;MACA,KAAKH,WAAL,GAAiBG,CAAjB;IACJ;;EAJI,CAXE;;EAiBVG,OAAO,GAAE;IACLb,MAAM,CAACc,GAAP,CAAW,QAAX,EACGC,IADH,CACQC,QAAO,IAAK;MAChBL,OAAO,CAACC,GAAR,CAAY,+BAAZ;MACA,KAAKT,KAAL,GAAaa,QAAQ,CAACd,IAAtB;MACA,MAAMe,UAAS,GAAI,IAAIC,IAAJ,CAAS,KAAKf,KAAL,CAAW,CAAX,CAAT,CAAnB;MACA,KAAKE,KAAL,GAAaY,UAAU,CAACE,cAAX,CAA0B,SAA1B,EAAqC;QAAEd,KAAK,EAAE;MAAT,CAArC,CAAb;MACAM,OAAO,CAACC,GAAR,CAAYK,UAAZ;MACA,KAAKX,GAAL,GAAWW,UAAU,CAACG,MAAX,EAAX;MACAT,OAAO,CAACC,GAAR,CAAY,KAAKP,KAAjB;MACAM,OAAO,CAACC,GAAR,CAAY,KAAKN,GAAjB;IACD,CAVH,EAWGe,KAXH,CAWSC,GAAE,IAAK;MACZX,OAAO,CAACC,GAAR,CAAYU,GAAZ;IACD,CAbH;EAcJ;;AAhCU,CAAd","names":["getAPI","name","data","dates","daysoftheWeek","month","day","selectedDay","methods","print","a","console","log","created","get","then","response","start_date","Date","toLocaleString","getDay","catch","err"],"sourceRoot":"","sources":["/Users/joshgoodman/Documents/GitHub/tententranslations/vuefrontend/src/components/Calender.vue"],"sourcesContent":["<template>\n<body v-if='dates[8]'>\n<h1>\n{{month}}\n</h1>\n<div id = 'calender'>\n    <div class = 'weekday' v-for='day in daysoftheWeek'>\n    {{day}}\n    </div>\n    <div v-for='d in day' :key = 'date'/>\n    <div class = 'cell' v-for='new Date(date) in dates' :key = 'date' @click='print(date)'>\n       <p>{{date.toLocaleString('default',{day:'numeric'})}}</p>\n    </div>\n</div>\n</body>\n</template>\n\n<script>\nimport { getAPI } from '../axios-api'\nexport default{\n    name:'Calender',\n    data(){\n        return {\n            dates:[],\n            daysoftheWeek:['Sunday','Monday','Tuesady','Wednesday','Thursday','Friday','Saturday'],\n            month:'',\n            day:0,\n            selectedDay:[]\n        }\n    },\n    methods:{\n        print(a){\n            console.log(a)\n            this.selectedDay=a\n        }\n    },\n    created(){\n        getAPI.get('/dates')\n          .then(response => {\n            console.log('Chapter API has recieved data')\n            this.dates = response.data\n            const start_date = new Date(this.dates[1])\n            this.month = start_date.toLocaleString('default', { month: 'long'})\n            console.log(start_date)\n            this.day = start_date.getDay()\n            console.log(this.month);\n            console.log(this.day)\n          })\n          .catch(err => {\n            console.log(err)\n          })\n    }\n\n}\n</script>\n<style scoped>\n.cell{\n    border:1px black solid;\n    width:100%;\n    height:100%;\n    margin:0px;\n}\n#calender{\n    width:100%;\n    height:100%;\n    min-height:700px;\n    display:grid;\n    grid-template-columns: auto auto auto auto auto auto auto;\n    gap:0px;\n    border:1px solid b