{"ast":null,"code":"import { getAPI } from '../axios-api';\nimport UtfBox from '../components/UtfBox';\nimport HomePageNovelCard from '../components/HomePageNovelCard';\nimport HomeCard from '../components/HomeCard';\nexport default {\n  name: 'HomePage',\n  props: ['tier'],\n  components: {\n    UtfBox,\n    HomePageNovelCard,\n    HomeCard\n  },\n\n  data() {\n    return {\n      popularNovels: [],\n      recentNovels: [],\n      latestNovels: []\n    };\n  },\n\n  created() {\n    getAPI.get('/home?tier=' + this.tier).then(response => {\n      console.log('Post Novel has recieved data');\n      this.popularNovels = response.data['popular'];\n      this.recentNovels = response.data['recent'];\n      this.latestNovels = response.data['latest'];\n      console.log(this.popularNovels);\n    }).catch(err => {\n      console.log(err);\n    });\n  }\n\n};","map":{"version":3,"mappings":"AAwCA,SAASA,MAAT,QAAuB,cAAvB;AACA,OAAOC,MAAP,MAAmB,sBAAnB;AACA,OAAOC,iBAAP,MAA8B,iCAA9B;AACA,OAAOC,QAAP,MAAqB,wBAArB;AACA,eAAc;EACVC,IAAI,EAAC,UADK;EAEVC,KAAK,EAAC,CACF,MADE,CAFI;EAKVC,UAAU,EAAC;IACPL,MADO;IAEPC,iBAFO;IAGPC;EAHO,CALD;;EAUVI,IAAI,GAAE;IACF,OAAM;MACFC,aAAa,EAAC,EADZ;MAEFC,YAAY,EAAC,EAFX;MAGFC,YAAY,EAAC;IAHX,CAAN;EAKH,CAhBS;;EAiBVC,OAAO,GAAE;IACLX,MAAM,CAACY,GAAP,CAAW,gBAAc,KAAKC,IAA9B,EACGC,IADH,CACQC,QAAO,IAAK;MAChBC,OAAO,CAACC,GAAR,CAAY,8BAAZ;MACA,KAAKT,aAAL,GAAqBO,QAAQ,CAACR,IAAT,CAAc,SAAd,CAArB;MACA,KAAKE,YAAL,GAAoBM,QAAQ,CAACR,IAAT,CAAc,QAAd,CAApB;MACA,KAAKG,YAAL,GAAoBK,QAAQ,CAACR,IAAT,CAAc,QAAd,CAApB;MACAS,OAAO,CAACC,GAAR,CAAY,KAAKT,aAAjB;IACD,CAPH,EAQGU,KARH,CAQSC,GAAE,IAAK;MACZH,OAAO,CAACC,GAAR,CAAYE,GAAZ;IACD,CAVH;EAWJ;;AA7BU,CAAd","names":["getAPI","UtfBox","HomePageNovelCard","HomeCard","name","props","components","data","popularNovels","recentNovels","latestNovels","created","get","tier","then","response","console","log","catch","err"],"sourceRoot":"","sources":["/Users/joshgoodman/Documents/GitHub/tententranslations/vuefrontend/src/views/HomePage.vue"],"sourcesContent":["<template>\n<div id = 'homePage' v-if='popularNovels[0]'>\n<div class = 'divide'>\n    <UtfBox shape = '&#x2B24;'/><h3 id = \"divider\">Most Popular</h3>\n</div><br>\n<div id = 'popular'>\n<HomePageNovelCard \n    :novelData='popularNovels[0]'\n    type=\"main\"\n    id = 'mainPopular'/>\n<div id = \"popularList\">\n        <div v-for='novel in popularNovels.slice(1,popularNovels.length)' :key = 'novel.title'>\n            <HomePageNovelCard \n                :novelData='novel' \n                class = 'popularnovel' \n                type = 'general'\n            />\n        </div>\n</div>\n</div>\n<div class = 'divide'>\n    <UtfBox shape = '&#x2B24;'/><h3 id = \"divider\">New Projects</h3>\n</div><br>\n<div class = \"novelrow\">\n        <div v-for='novel in recentNovels' :key = 'novel.title'>\n            <HomeCard :novelData='novel' type = \"home\"/>\n        </div>\n</div>\n<div class = 'divide'>\n    <UtfBox shape = '&#x2B24;'/><h3 id = \"divider\">Latest Releases</h3>\n</div><br>\n<div class = \"novelrow\">\n        <div v-for='novel in latestNovels' :key = 'novel.title'>\n            <HomeCard :novelData='novel' type = \"home\"/>\n        </div>\n</div>\n</div>\n</template>\n\n<script>\nimport { getAPI } from '../axios-api'\nimport UtfBox from '../components/UtfBox'\nimport HomePageNovelCard from '../components/HomePageNovelCard'\nimport HomeCard from '../components/HomeCard'\nexport default{\n    name:'HomePage',\n    props:[\n        'tier'\n    ],\n    components:{\n        UtfBox,\n        HomePageNovelCard,\n        HomeCard\n    },\n    data(){\n        return{\n            popularNovels:[],\n            recentNovels:[],\n            latestNovels:[]\n        }\n    },\n    created(){\n        getAPI.get('/home?tier='+this.tier)\n          .then(response => {\n            console.log('Post Novel has recieved data')\n            this.popularNovels = response.data['popular']\n            this.recentNovels = response.data['recent']\n            this.latestNovels = response.data['latest']\n            console.log(this.popularNovels)\n          })\n          .catch(err => {\n            console.log(err)\n          })\n    }\n}\n\n</script>\n<style scoped>\n#homePage{\n    width:100%;\n}\nh3{\n    font-size:20px;\n    margin-left:10px;\n}\n.divide{\n    display:flex;\n    flex-direction:row;\n    border-bottom:1px var(--borderColor) solid;\n    height:30px;\n    width:100%;\n    padding-bottom:35px;\n}\n#popularList{\n   display: grid;\n   grid-template-columns: auto auto auto auto;\n   margin-right:auto;\n   height:fit-content;\n   \n}\n.novelrow{\n    display: flex;\n    flex-direction:row;\n    margin:auto;\n    height:fit-content;\n}\n#popular{\n    display:flex;\n    flex-direction:row;\n}\n@media (max-width: 1200px){\n     .novelrow{\n         display:grid;\n         grid-template-columns: auto auto auto;\n    }\n    #popular{\n        display:inline-bloxk;\n    }\n  }\n@media (max-width: 775px) {\n    .novelrow{\n         grid-template-columns: auto auto;\n         margin:0px;\n    }\n    #popularList{\n        grid-template-columns: auto;\n       margin-left:10px;\n    }\n   \n}\n</style>"]},"metadata":{},"sourceType":"module"}