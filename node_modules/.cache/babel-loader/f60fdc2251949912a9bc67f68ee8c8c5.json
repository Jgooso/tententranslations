{"ast":null,"code":"import { getAPI } from '../axios-api';\nexport default {\n  name: 'Feedback',\n\n  data() {\n    return {\n      page: 1,\n      feedback: []\n    };\n  },\n\n  methods: {\n    getFeedback(change) {\n      this.page = this.page + change;\n      getAPI.get('/feedback?page=' + this.page).then(response => {\n        console.log('Post Novel has recieved data');\n        this.feedback = response.data;\n        console.log(this.feedback);\n      }).catch(err => {\n        console.log(err);\n      });\n    },\n\n    deleteFeedback(id) {\n      console.log(id);\n      getAPI.delete('/feedback?id=' + id).then(response => {\n        console.log('Deleted Feedback');\n      }).catch(err => {\n        console.log(err);\n      });\n    }\n\n  },\n\n  created() {\n    this.getFeedback(0);\n  }\n\n};","map":{"version":3,"mappings":"AAcA,SAASA,MAAT,QAAuB,cAAvB;AACA,eAAc;EACVC,IAAI,EAAC,UADK;;EAEVC,IAAI,GAAE;IACF,OAAM;MACFC,IAAI,EAAC,CADH;MAEFC,QAAQ,EAAC;IAFP,CAAN;EAIH,CAPS;;EAQVC,OAAO,EAAC;IACJC,WAAW,CAACC,MAAD,EAAQ;MACf,KAAKJ,IAAL,GAAY,KAAKA,IAAL,GAAYI,MAAxB;MACAP,MAAM,CAACQ,GAAP,CAAW,oBAAkB,KAAKL,IAAlC,EACDM,IADC,CACIC,QAAO,IAAK;QAChBC,OAAO,CAACC,GAAR,CAAY,8BAAZ;QACA,KAAKR,QAAL,GAAgBM,QAAQ,CAACR,IAAzB;QACAS,OAAO,CAACC,GAAR,CAAY,KAAKR,QAAjB;MACD,CALC,EAMDS,KANC,CAMKC,GAAE,IAAK;QACZH,OAAO,CAACC,GAAR,CAAYE,GAAZ;MACD,CARC;IASH,CAZG;;IAaJC,cAAc,CAACC,EAAD,EAAI;MACdL,OAAO,CAACC,GAAR,CAAYI,EAAZ;MACAhB,MAAM,CAACiB,MAAP,CAAc,kBAAgBD,EAA9B,EAAkCP,IAAlC,CAAuCC,QAAO,IAAI;QAC9CC,OAAO,CAACC,GAAR,CAAY,kBAAZ;MACH,CAFD,EAEGC,KAFH,CAESC,GAAE,IAAI;QACXH,OAAO,CAACC,GAAR,CAAYE,GAAZ;MAEH,CALD;IAMJ;;EArBI,CARE;;EA+BVI,OAAO,GAAE;IACL,KAAKZ,WAAL,CAAiB,CAAjB;EACJ;;AAjCU,CAAd","names":["getAPI","name","data","page","feedback","methods","getFeedback","change","get","then","response","console","log","catch","err","deleteFeedback","id","delete","created"],"sourceRoot":"","sources":["/Users/joshgoodman/Documents/GitHub/tententranslations/vuefrontend/src/views/Feedback.vue"],"sourcesContent":["<template>\n    <div id = 'feedback'>\n        <div v-for='f in feedback'  :key = 'f.id' class = 'feedbackbox'>\n            <p>{{f.feedback}}</p>\n            <button class = 'delete-feedback' @click='deleteFeedback(f.id)'>X</button>\n        </div>\n        <nav id = 'page-navigation'>\n         <button class = 'page-button' id = 'left-arrow'>&larr;</button>\n        <button class = 'page-button' id = 'right-arrow'>&rarr;</button>\n        </nav>\n    </div>\n</template>\n\n<script>\nimport { getAPI } from '../axios-api'\nexport default{\n    name:'Feedback',\n    data(){\n        return{\n            page:1,\n            feedback:[]\n        }\n    },\n    methods:{\n        getFeedback(change){\n            this.page = this.page + change\n            getAPI.get('/feedback?page='+this.page)\n          .then(response => {\n            console.log('Post Novel has recieved data')\n            this.feedback = response.data\n            console.log(this.feedback)\n          })\n          .catch(err => {\n            console.log(err)\n          })\n        },\n        deleteFeedback(id){\n            console.log(id)\n            getAPI.delete('/feedback?id='+id).then(response =>{\n                console.log('Deleted Feedback')\n            }).catch(err =>{\n                console.log(err)\n\n            })\n        }\n    },\n    created(){\n        this.getFeedback(0)\n    }\n}\n</script>\n<style scoped>\n#feedback{\n    display:flex;\n    flex-direction:row;\n}\n.feedbackbox{\n    border: 1px solid var(--borderColor);\n    width: 100%;\n    margin-bottom:50px;\n    height:200px;\n    overflow:scroll;\n    margin-left:20px;\n    margin-right:20px;\n}\n.delete-feedback{\n    border:none;\n    background:transparent;\n    position:relative;\n    top:20px;\n    right:20px;\n}\n.page-button{\n    border:none;\n    background-color:rgba(200,200,200,0.2);\n    width: 50px;\n    height:50px;\n}\n#page-navigation{\n    margin:auto;\n    width:200px;\n}\n\n#right-arrow{\n    margin-left:100px;\n}\n</style>"]},"metadata":{},"sourceType":"module"}