  Footer,\n    sidebar\n  },\n  data(){\n    return{\n    tier: 3,\n    darkmore:false,\n    testing:[],\n    }\n  },\n  methods:{\n        changesidebar(){\n          const menuBtn = this.$refs.header.$refs.menubutton;\n          if(this.$refs.sidemenu.$refs.sidebar.style.left=='0px'){\n            this.$refs.sidemenu.$refs.sidebar.style.left='-300px';\n            this.$refs.sidemenu.$refs.shade.style.opacity='0';\n            menuBtn.classList.remove('open');\n          }else{\n            this.$refs.sidemenu.$refs.sidebar.style.left='0px';\n            this.$refs.sidemenu.$refs.shade.style.opacity='0.5';\n            menuBtn.classList.add('open');\n          }\n          \n        },\n        darkmode(){\n          if(darkmode){\n            document.style.color='white'\n            document.body.style.backgroundColor='black'\n          }\n        }\n            \n        },\n    errorCaptured: function(err) {\n      this.$router.push({name:'errorPage'})\n      \n    },\n    created(){\n      getAPI.get('/?name=joe')\n          .then(response => {\n            console.log('Post API has recieved data')\n            console.log(repsonse)\n          })\n          .catch(err => {\n            console.log(err)\n          })\n    }\n  \n}\n</script>\n\n<style>\n*{\n  font:Arial, Helvetica;\n  color:black;\n}\n  body {\n    margin: 0;\n    padding: 0;\n    background-color:white!important;\n    \n  }\n  .content{\n      width: 1110px;\n      margin:auto;\n      position:relative;\n      height: 100%;\n      background-color:rgba(0,0,0,0)\n  }\n  \n\n  @media (max-width: 1200px){\n     .content{\n         width: 92.5%;\n         transition: all .3s ease;\n     }\n  }\n\n@media (max-width: 775px) {\n    .content{\n        display: flex;\n        flex-direction: column;\n        margin-top: 30px;\n        margin: auto;\n        transition: all .3s ease;\n    }\n}\n</style>\n"]},"metadata":{},"sourceType":"module"}