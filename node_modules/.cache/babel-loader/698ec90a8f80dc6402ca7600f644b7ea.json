{"ast":null,"code":"import Header from './components/Header';\nimport Footer from './components/Footer';\nimport sidebar from './components/Sidebar';\nimport { getAPI } from './axios-api';\nexport default {\n  name: 'App',\n  components: {\n    Header,\n    Footer,\n    sidebar\n  },\n\n  data() {\n    return {\n      tier: 3,\n      darkmore: false,\n      testing: []\n    };\n  },\n\n  methods: {\n    changesidebar() {\n      const menuBtn = this.$refs.header.$refs.menubutton;\n\n      if (this.$refs.sidemenu.$refs.sidebar.style.left == '0px') {\n        this.$refs.sidemenu.$refs.sidebar.style.left = '-300px';\n        this.$refs.sidemenu.$refs.shade.style.opacity = '0';\n        menuBtn.classList.remove('open');\n      } else {\n        this.$refs.sidemenu.$refs.sidebar.style.left = '0px';\n        this.$refs.sidemenu.$refs.shade.style.opacity = '0.5';\n        menuBtn.classList.add('open');\n      }\n    },\n\n    darkmode() {\n      if (darkmode) {\n        document.style.color = 'white';\n        document.body.style.backgroundColor = 'black';\n      }\n    }\n\n  },\n  errorCaptured: function (err) {\n    this.$router.push({\n      name: 'errorPage'\n    });\n  },\n\n  created() {\n    getAPI.get('/?name=joe').then(response => {\n      console.log('Post API has recieved data');\n      console.log(repsonse);\n    }).catch(err => {\n      console.log(err);\n    });\n  }\n\n};","map":{"version":3,"mappings":"AAkBA,OAAOA,MAAP,MAAmB,qBAAnB;AACA,OAAOC,MAAP,MAAmB,qBAAnB;AACA,OAAOC,OAAP,MAAoB,sBAApB;AACA,SAASC,MAAT,QAAuB,aAAvB;AACA,eAAe;EAEbC,IAAI,EAAE,KAFO;EAGbC,UAAU,EAAC;IACTL,MADS;IAETC,MAFS;IAGTC;EAHS,CAHE;;EAQbI,IAAI,GAAE;IACJ,OAAM;MACNC,IAAI,EAAE,CADA;MAENC,QAAQ,EAAC,KAFH;MAGNC,OAAO,EAAC;IAHF,CAAN;EAKD,CAdY;;EAebC,OAAO,EAAC;IACFC,aAAa,GAAE;MACb,MAAMC,OAAM,GAAI,KAAKC,KAAL,CAAWC,MAAX,CAAkBD,KAAlB,CAAwBE,UAAxC;;MACA,IAAG,KAAKF,KAAL,CAAWG,QAAX,CAAoBH,KAApB,CAA0BX,OAA1B,CAAkCe,KAAlC,CAAwCC,IAAxC,IAA8C,KAAjD,EAAuD;QACrD,KAAKL,KAAL,CAAWG,QAAX,CAAoBH,KAApB,CAA0BX,OAA1B,