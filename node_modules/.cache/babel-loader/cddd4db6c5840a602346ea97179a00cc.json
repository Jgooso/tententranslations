{"ast":null,"code":"import Header from './components/Header';\nimport Footer from './components/Footer';\nimport sidebar from './components/Sidebar';\nexport default {\n  name: 'App',\n  components: {\n    Header,\n    Footer,\n    sidebar\n  },\n\n  data() {\n    return {\n      tier: 3,\n      darkmore: false\n    };\n  },\n\n  methods: {\n    changesidebar() {\n      const menuBtn = this.$refs.header.$refs.menubutton;\n\n      if (this.$refs.sidemenu.$refs.sidebar.style.left == '0px') {\n        this.$refs.sidemenu.$refs.sidebar.style.left = '-300px';\n        this.$refs.sidemenu.$refs.shade.style.opacity = '0';\n        menuBtn.classList.remove('open');\n      } else {\n        this.$refs.sidemenu.$refs.sidebar.style.left = '0px';\n        this.$refs.sidemenu.$refs.shade.style.opacity = '0.5';\n        menuBtn.classList.add('open');\n      }\n    },\n\n    darkmode() {\n      if (darkmode) {\n        document.style.color = 'white';\n        document.body.style.backgroundColor = 'black';\n      }\n    }\n\n  },\n  errorCaptured: function (err) {\n    this.$router.push({\n      name: 'errorPage'\n    });\n  }\n};","map":{"version":3,"mappings":"AAkBA,OAAOA,MAAP,MAAmB,qBAAnB;AACA,OAAOC,MAAP,MAAmB,qBAAnB;AACA,OAAOC,OAAP,MAAoB,sBAApB;AACA,eAAe;EAEbC,IAAI,EAAE,KAFO;EAGbC,UAAU,EAAC;IACTJ,MADS;IAETC,MAFS;IAGTC;EAHS,CAHE;;EAQbG,IAAI,GAAE;IACJ,OAAM;MACNC,IAAI,EAAE,CADA;MAENC,QAAQ,EAAC;IAFH,CAAN;EAID,CAbY;;EAcbC,OAAO,EAAC;IACFC,aAAa,GAAE;MACb,MAAMC,OAAM,GAAI,KAAKC,KAAL,CAAWC,MAAX,CAAkBD,KAAlB,CAAwBE,UAAxC;;MACA,IAAG,KAAKF,KAAL,CAAWG,QAAX,CAAoBH,KAApB,CAA0BT,OAA1B,CAAkCa,KAAlC,CAAwCC,IAAxC,IAA8C,KAAjD,EAAuD;QACrD,KAAKL,KAAL,CAAWG,QAAX,CAAoBH,KAApB,CAA0BT,OAA1B,CAAkCa,KAAlC,CAAwCC,IAAxC,GAA6C,QAA7C;QACA,KAAKL,KAAL,CAAWG,QAAX,CAAoBH,KAApB,CAA0BM,KAA1B,CAAgCF,KAAhC,CAAsCG,OAAtC,GAA8C,GAA9C;QACAR,OAAO,CAACS,SAAR,CAAkBC,MAAlB,CAAyB,MAAzB;MACD,CAJD,MAIK;QACH,KAAKT,KAAL,CAAWG,QAAX,CAAoBH,KAApB,CAA0BT,OAA1B,CAAkCa,KAAlC,CAAwCC,IAAxC,GAA6C,KAA7C;QACA,KAAKL,KAAL,CAAWG,QAAX,CAAoBH,KAA