{"ast":null,"code":"import KoFiButton from '../components/KoFiButton';\nimport NavButton from '../components/Navbutton';\nimport axios from 'axios';\nexport default {\n  name: 'Chapter',\n  components: {\n    KoFiButton,\n    NavButton\n  },\n  props: ['novelData', 'chapterList'],\n\n  data() {\n    return {\n      chapter: [],\n      chapterContent: null,\n      error: null,\n      chapterTitle: null\n    };\n  },\n\n  methods: {\n    changeFontSize(change) {\n      const v = document.getElementById('chapter-content');\n      const newFontSize = parseInt(getComputedStyle(v).getPropertyValue('font-size').slice(0, 2)) + change;\n\n      if (newFontSize > 6) {\n        v.style.fontSize = newFontSize + 'px';\n        $cookies.set('font-size', newFontSize + 'px');\n      }\n    },\n\n    switchdarkmode() {\n      this.$emit(\"switchmode\");\n    },\n\n    setData(content, title) {\n      this.chapterContent = content;\n      this.chapterTitle = title;\n    }\n\n  },\n\n  created() {\n    this.chapter = this.$route.params;\n    const url = `http://tententranslation.com/chapter?novel=${this.chapter.title}&chapter=${this.chapter.chapter}`;\n    axios.get(url).then(response => {\n      this.chapterContent = response.data['content'];\n      this.chapterTitle = response.data['title'];\n    }).catch(err => {\n      console.log(err);\n    });\n  },\n\n  mounted() {\n    const v = document.getElementById('chapter-content');\n\n    if ($cookies.get('font-size') == null) {\n      $cookies.set('font-size', '19px');\n      v.style.fontSize = '19px';\n    } else {\n      v.style.fontSize = $cookies.get('font-size');\n    }\n  }\n\n};","map":{"version":3,"mappings":"AA8BA,OAAOA,UAAP,MAAuB,0BAAvB;AACA,OAAOC,SAAP,MAAsB,yBAAtB;AACA,OAAOC,KAAP,MAAkB,OAAlB;AACI,eAAc;EACVC,IAAI,EAAE,SADI;EAEVC,UAAU,EAAC;IACPJ,UADO;IAEPC;EAFO,CAFD;EAKRI,KAAK,EAAC,CACR,WADQ,EAER,aAFQ,CALE;;EAQRC,IAAI,GAAE;IACJ,OAAM;MACFC,OAAO,EAAC,EADN;MAEFC,cAAc,EAAC,IAFb;MAGFC,KAAK,EAAC,IAHJ;MAIFC,YAAY,EAAC;IAJX,CAAN;EAMH,C