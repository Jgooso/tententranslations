{"ast":null,"code":"import ChapterSelector from '../components/ChapterSelector';\nimport Navigator from '../components/Navigator';\nimport { getAPI } from '../axios-api';\nexport default {\n  name: 'Chapter',\n  components: {\n    ChapterSelector,\n    Navigator\n  },\n  props: ['novelData', 'chapterList'],\n\n  data() {\n    return {\n      chapter: []\n    };\n  },\n\n  methods: {\n    updateContent(chp) {\n      this.chapterNum = chp - 1;\n      window.scrollTo({\n        top: 0,\n        left: 0,\n        behavoir: 'smooth'\n      });\n    },\n\n    changeFontSize(change) {\n      if (change == 1) {\n        document.getElementsByTagName('pre')[0].style.fontSize = 'larger';\n      } else {\n        document.getElementsByTagName('pre')[0].style.fontSize = 'smaller';\n      }\n    }\n\n  },\n\n  created() {\n    const url = '/chapter?novel=' + this.$route.params.title + '&chapter=' + this.$route.params.chapter;\n    getAPI.get(url).then(response => {\n      console.log('Post API has recieved data');\n      this.chapter = response;\n    }).catch(err => {\n      console.log(err);\n    });\n    this.chapterList.sort((a, b) => a.chapterNumber > b.chapterNumber ? 1 : -1);\n  }\n\n};","map":{"version":3,"mappings":"AA2BA,OAAOA,eAAP,MAA4B,+BAA5B;AACA,OAAOC,SAAP,MAAsB,yBAAtB;AACA,SAASC,MAAT,QAAuB,cAAvB;AACI,eAAc;EACVC,IAAI,EAAE,SADI;EAEVC,UAAU,EAAC;IACPJ,eADO;IAEPC;EAFO,CAFD;EAKRI,KAAK,EAAC,CACR,WADQ,EAER,aAFQ,CALE;;EASVC,IAAG,GAAK;IACJ,OAAO;MACHC,OAAO,EAAC;IADL,CAAP;EAGH,CAbS;;EAaRC,OAAO,EAAC;IACVC,aAAa,CAACC,GAAD,EAAK;MACd,KAAKC,UAAL,GAAkBD,GAAG,GAAC,CAAtB;MACAE,MAAM,CAACC,QAAP,CAAgB;QAACC,GAAG,EAAC,CAAL;QAAOC,IAAI,EAAC,CAAZ;QAAcC,QAAQ,EAAC;MAAvB,CAAhB;IAEH,CALS;;IAMVC,cAAc,CAACC,MAAD,EACd;MACI,IAAGA,MAAK,IAAK,CAAb,EAAe;QACXC,QAAQ,CAACC,oBAAT,CAA8B,KAA9B,EAAqC,CAArC,EAAwCC,KAAxC,CAA8CC,QAA9C,GAAuD,QAAvD;MACH,CAFD,MAEK;QACDH,QAAQ,CAACC,oBAAT,CAA8B,KAA9B,EAAqC,CAArC,EAAwCC,KAAxC,CAA8CC,QAA9C,GAAuD,SAAvD;MACJ;IACJ;;EAbU,CAbA;;EA4BdC,OAAO,GAAE;IACL,MAAMC,GAAE,GAAI,oBAAkB,KA