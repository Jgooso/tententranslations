{"ast":null,"code":"import { getAPI } from '../axios-api';\nexport default {\n  name: 'ScheduleCreator',\n  methods: {\n    submit() {\n      const time = document.getElementById('appt').value;\n      const novel = document.getElementById('novel-selector').value;\n      console.log(time);\n      console.log(novel);\n      const dates = document.getElementsByClassName('date-selector');\n      var selecteddates = [];\n      console.log(dates.length);\n\n      for (var i = 0; i < dates.length; i++) {\n        if (dates[i].checked == true) {\n          selecteddates.push(dates[i].value);\n        }\n      }\n\n      console.log(selecteddates);\n      getAPI.post('/schedule', {\n        novel: novel,\n        time: time,\n        dates: selecteddates\n      }).then(function (response) {\n        console.log(response);\n      }).catch(function (error) {\n        console.log(error);\n      });\n      console.log('what');\n    }\n\n  },\n\n  data() {\n    return {\n      novels: []\n    };\n  },\n\n  created() {\n    getAPI.get('/noveltitles').then(response => {\n      console.log('Chapter API has recieved data');\n      this.novels = response.data;\n      console.log(response.data);\n    }).catch(err => {\n      console.log(err);\n    });\n  }\n\n};","map":{"version":3,"mappings":"AAqBA,SAASA,MAAT,QAAuB,cAAvB;AACA,eAAc;EACVC,IAAI,EAAC,iBADK;EAEVC,OAAO,EAAC;IACJC,MAAM,GAAE;MACJ,MAAMC,IAAG,GAAIC,QAAQ,CAACC,cAAT,CAAwB,MAAxB,EAAgCC,KAA7C;MACA,MAAMC,KAAI,GAAIH,QAAQ,CAACC,cAAT,CAAwB,gBAAxB,EAA0CC,KAAxD;MACAE,OAAO,CAACC,GAAR,CAAYN,IAAZ;MACAK,OAAO,CAACC,GAAR,CAAYF,KAAZ;MACA,MAAMG,KAAI,GAAIN,QAAQ,CAACO,sBAAT,CAAgC,eAAhC,CAAd;MACA,IAAIC,aAAY,GAAI,EAApB;MACAJ,OAAO,CAACC,GAAR,CAAYC,KAAK,CAACG,MAAlB;;MACA,KAAI,IAAIC,CAAC,GAAC,CAAV,EAAaA,IAAIJ,KAAK,CAACG,MAAvB,EAA+BC,CAAC,EAAhC,EAAmC;QAC/B,IAAGJ,KAAK,CAACI,CAAD,CAAL,CAASC,OAAT,IAAkB,IAArB,EAA0B;UACtBH,aAAa,CAACI,IAAd,CAAmBN,KAAK,CAACI,CAAD,CAAL,CAASR,KAA5B;QACJ;MACJ;;MACAE,OAAO,CAACC,GAAR,CAAYG,aAAZ;MACAb,MAAM,CAACkB,IAAP,CAAY,WAAZ,EAAy