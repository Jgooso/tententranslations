ter){\n              this.$router.push({name: 'chapterPage', params:{title:this.$route.params.title,chapter:chapter}})\n            }\n\n        }\n    }\n</script>\n<style scoped>\n#Button.NavButton {\n    cursor: pointer;\n    background: purple;\n    color: white;\n    font-size: 12px;\n    border: 0;\n    padding: 0.75rem 1rem;\n    border-radius: 5px;\n    font-weight: bold;\n    float:center;\n    margin-right: 10px;\n  }\n  \n  #Button:hover {\n    transform: scale(0.98);\n    transform: scale(0.98);\n  }\n  #Button.MoveButton {\n    cursor: pointer;\n    background: purple;\n    color: white;\n    font-size: 17px;\n    font-weight: 300;\n    border: 0;\n    padding: 0.75rem 1rem;\n    padding-left:0px;\n    padding-right:0;\n    align-self:center;\n    border-radius: 5px;\n    margin-right: 2px;\n    width:84px;\n    \n  }\n  #MoveButton{\n    position:absolute;\n    right:0px;\n  }\n\n  </style>"]},"metadata":{},"sourceType":"module"}